    <description>SFT-428 - Set AB testing, change template, send email on closure</description>
    <environments>Default</environments>
    <formulas>
        <name>FormulaModCurrentMinute</name>
        <dataType>Number</dataType>
        <expression>MOD( (NOW() - DATETIMEVALUE( &quot;1970-01-01 00:00:00&quot; ))  * 86400 ,2)</expression>
        <scale>2</scale>
    </formulas>
