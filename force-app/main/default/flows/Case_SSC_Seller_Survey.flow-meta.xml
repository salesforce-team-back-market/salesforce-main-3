            </connector>
            <label>SSC cases</label>
        </rules>
    </decisions>
    <decisions>
        <name>Should_we_send_FR_EU_or_US_template</name>
        <label>Should we send FR, EU or US template?</label>
        <locationX>908</locationX>
        <locationY>575</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>FR</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Account.Zone__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>fr-fr</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>FR_AB_testing</targetReference>
            </connector>
            <label>FR</label>
        </rules>
        <rules>
            <name>EU</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Account.Zone__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>fr-fr</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Account.Zone__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>en-us</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Account.Zone__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>jp-jp</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>EU_AB_testing</targetReference>
            </connector>
            <label>EU</label>
        </rules>
        <rules>
            <name>US</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Account.Zone__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>en-us</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>US_AB_testing</targetReference>
            </connector>
            <label>US</label>
        </rules>
    </decisions>
