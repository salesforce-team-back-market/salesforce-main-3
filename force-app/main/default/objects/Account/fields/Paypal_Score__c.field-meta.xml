<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Paypal_Score__c</fullName>
    <externalId>false</externalId>
    <formula>IF ( ISPICKVAL ( Stage__c,  &apos;Offline&apos;), 
     0, 
     Number_of_Live_Paypal_Targeted_Countries__c )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Number of Live Paypal Targeted Countries divided by the total number of Targeted Countries for this Merchant if it&apos;s Online. Otherwise, zero.</inlineHelpText>
    <label>Paypal Score</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
